<?xml version='1.0' encoding='utf-8'?>
<widget id="com.deen812.hoursoflife" version="1.0.3" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="LocalNotification">
        <param name="android-package" value="de.appplant.cordova.plugin.localnotification.LocalNotification" />
    </feature>
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Badge">
        <param name="android-package" value="de.appplant.cordova.plugin.badge.Badge" />
    </feature>
    <name>Часы жизни</name>
    <description>
        hours of you life
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        DeenTeam
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon density="mdpi" height="72" platform="android" src="res/icon64.png" width="72" />
    <allow-intent href="market:*" />
    <splash density="land-hdpi" src="res/splash-land-hdpi.png" />
    <splash density="land-ldpi" src="res/splash1.png" />
    <splash density="land-mdpi" src="res/splash-land-mdpi.png" />
    <splash density="land-xhdpi" src="res/splash-land-xhdpi.png" />
    <splash density="port-hdpi" src="res/splash-port-hdpi.png" />
    <splash density="port-ldpi" src="res/splash1.png" />
    <splash density="port-mdpi" src="res/splash-port-mdpi.png" />
    <splash density="port-xhdpi" src="res/splash-port-xhdpi.png" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="android-targetSdkVersion" value="28" />
</widget>
